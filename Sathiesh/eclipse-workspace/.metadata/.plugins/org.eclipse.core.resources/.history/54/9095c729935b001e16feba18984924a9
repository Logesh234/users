package Collection;

import java.util.PriorityQueue;

public class QueueExamples {

	public static void main(String[] args) {
		PriorityQueue<Integer>p=new PriorityQueue<Integer>();
		p.add(12);
		p.add(13);
		p.add(15);
		p.add(67);
		p.add(90);
		System.out.println(p);
		System.out.println(p.peek());//retrieve the first data
		System.out.println(p.poll());//retrieve the first data
		System.out.println(p);
		System.out.println(p.element());//retrieve the first data
		System.out.println(p.remove());//retrieve the first data
		
		System.out.println(p);
	

	}

}
